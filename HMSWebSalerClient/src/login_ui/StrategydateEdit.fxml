            <TextField fx:id="discountField" GridPane.columnIndex="1" GridPane.rowIndex="3" />
         </children>
      </GridPane>
      <Button fx:id="confirm" layoutX="111.0" layoutY="238.0" mnemonicParsing="false" onAction="#onSaveClicked" prefHeight="23.0" prefWidth="73.0" text="保存更改" />
